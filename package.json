{
    "name": "debug-info",
    "version": "0.0.4",
    "description": "Just a plugin for debugging some stuff",
    "main": "./index.ts",
    "scripts": {
        "archive": "rimraf *.jspz && npm-pack-zip",
        "build": "ncc build ./index.ts -o ./dist/ --minify --no-cache",
        "postarchive": "renamer --find zip --replace jspz *",
        "prettier": "prettier --config .prettierrc src/**/*.* --check",
        "prettier:format": "prettier --config .prettierrc src/**/*.* --write"
    },
    "prismarine": {
        "apiVersion": "1.0",
        "displayName": "DebugInfo"
    },
    "author": "@JSPrismarine",
    "license": "MIT",
    "dependencies": {
        "@jsprismarine/brigadier": "^0.1.0",
        "@jsprismarine/prismarine": "latest",
        "common-tags": "^1.8.0",
        "fs": "0.0.1-security",
        "path": "^0.12.7",
        "typescript": "^4.1.3"
    },
    "devDependencies": {
        "@vercel/ncc": "0.24.1",
        "npm-pack-zip": "1.2.7",
        "prettier": "^2.2.1",
        "renamer": "2.0.1",
        "rimraf": "^3.0.2"
    }
}
